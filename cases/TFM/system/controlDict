FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Make sure all utilities know specialised models
libs            (atmosphericModels);

application     twoPhaseEulerFoam;

startTime       0;

stopAt          endTime;

endTime         3000;

deltaT          0.01;

writeControl    runTime;

writeInterval   10;

purgeWrite      10;

writeFormat     ascii;

writePrecision  12;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable no;

functions
{
    #include "sample"
}


